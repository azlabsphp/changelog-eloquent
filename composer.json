{
    "name": "drewlabs/changelog-eloquent",
    "description": "eloquent driver for drewlabs/changelog library",
    "type": "library",
    "require": {
        "drewlabs/changelog": "^0.1.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Changelog\\Eloquent\\": "src/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "azandrewdevelopper@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
